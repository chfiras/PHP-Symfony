<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

{% form_theme form _self %}
  {{ form_start(form, {'attr': {'class': 'form-horizontal', 'id' : 'myForm'}}) }}
<div class="block">
    <div class="block-title">
        <h2>Ajout d'un pack</h2>
    </div>
    <div class="form-group">
        {{ form_label(form.name,'Nom du pack',{'label_attr': {'class': 'col-md-2 control-label'}}) }}
        {{ form_widget(form.name) }}
    </div>
    <div class="form-group">
        {{ form_label(form.description,'Description',{'label_attr': {'class': 'col-md-1 control-label'}}) }}
    {{ form_widget(form.description) }}
    </div>
    {{ form_widget(form.save, {'attr': {'class': 'btn btn-primary'}}) }}
    <br><br>


</div>


{{ form_end(form) }}




